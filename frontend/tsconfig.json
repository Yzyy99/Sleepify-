{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["ES2017"],
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    //"target": "ES2015",
    "allowJs": false,
    "experimentalDecorators": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "removeComments": true,
    "pretty": true,
    "strictPropertyInitialization": true,
    "typeRoots": [
      "./typings",
      "node_modules/@types"
    ]
  },
  "include": [
    "./**/*.ts"
, "miniprogram/pages/music2/music2.ts"  ],
  "exclude": [
    "node_modules"
  ]
}